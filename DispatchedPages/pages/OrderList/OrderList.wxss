page{
    background-color: #f5f5f5;
  }
.navbar{
    flex: none;
    display: flex;
    background: #fff;
  }
.navbar .item{
    position: relative;
    flex: auto;
    text-align: center;
    line-height: 24rpx;
}
.navbar .item.active{
    color: #2269d4;
    height: 80rpx;
}
.navbar .item.active:after{
    content: "";
    display: block;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 4rpx;
    background: #2269d4;
}
.NewSingle{
    background-color: #fff;
    margin-top:22rpx;
    padding: 0 20rpx;
    font-size: 28rpx;
}
.NewSingle_top{
    height: 90rpx;
    line-height: 90rpx;
    display: flex;
    justify-content: space-between;
    border-bottom:2rpx solid #f5f5f5;
}
.NewSingle_middle{
    padding: 20rpx 0;
    border-bottom:2rpx solid #f5f5f5;
}
.NewSingle_bottom{
    padding: 20rpx 0;
    display: flex;
    justify-content: flex-end;
}
.NewSingle_bottom>view{
    width: 160rpx;
    height: 62rpx;
    color: #fff;
    line-height: 62rpx;
    text-align: center;
    border-radius: 50rpx;
}
.remove{
    color: #2269d4;
    position: relative;
    top: -78rpx;
    left: 546rpx;
}


.modal-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
    z-index: 9000;
    color: #fff;
  }
  
  .modal-dialog {
    width: 680rpx;
    overflow: hidden;
    position: fixed;
    top: 50%;
    left: 0;
    z-index: 9999;
    background: #f9f9f9;
    margin: -180rpx 34rpx;
    border-radius: 36rpx;
  }
  .modal-title {
    /* padding-top: 42rpx; */
    font-size: 36rpx;
    color: #030303;
    text-align: center;
  }
  .btn-Photograph{
    width: 680rpx;
    height: 81rpx;
    text-align: center;
    line-height: 89rpx;
    color: #fff;
    background-color: #2269D4 ;
    font-size: 46rpx;
    font-weight: 900;
  }
  .modal-footer {
    display: flex;
    flex-direction: row;
    height: 86rpx;
    border-top: 1px solid #dedede;
    font-size: 34rpx;
    line-height: 86rpx;
  }
  
  .btn-cancel {
    width: 50%;
    color: #007aff;
    text-align: center;
    border-right: 1px solid #dedede;
  }
  .btn-confirm {
    width: 50%;
    color: #007aff;
    text-align: center;
  }
  .label-2{
    padding: 0 30rpx;
    display: flex;
    justify-content: space-between;
  }
