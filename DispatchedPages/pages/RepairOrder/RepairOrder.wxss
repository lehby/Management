.form{
    padding: 0 20rpx;
    background-color: #fff;
    font-size: 28rpx;
    margin-top: 22rpx;
    
}
.form_top{
    height: 80rpx;
    line-height: 80rpx;
    display:flex;
    justify-content: space-between;
    border-bottom: 2rpx solid #f5f5f5;
}
.form_middle{
    padding: 34rpx 0;
    border-bottom: 2rpx solid #f5f5f5;
}
.NewSingle_bottom{
    padding: 20rpx 0;
    display: flex;
    justify-content: flex-end;
}
.NewSingle_bottom>view{
    width: 160rpx;
    height: 62rpx;
    color: #fff;
    line-height: 62rpx;
    text-align: center;
    border-radius: 50rpx;
}

/* 底部导航条 */

.fonter_head {
  display: flex;
  justify-content: space-around;
  background-color: white;
  width: 750rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  padding: 6rpx;
}

.fonter {
  display: flex;
  flex-direction: column;
  font-size: 25rpx;
  align-items: center;
}

.fonter image {
  width: 60rpx;
  height: 60rpx;
}