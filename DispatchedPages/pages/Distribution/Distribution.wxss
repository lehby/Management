.occupy{
    height: 1026rpx;
}
/* 地图 */
.section{
    height: 30px;
    width: 100%;
  }
  .section input{
    width:90%;
    margin:5px auto;
    border:1px solid #c3c3c3;
    height:30px;
    border-radius: 3px;
    padding: 0 5px;
  }
  .map_container{
    position: absolute;
    top: 0px;
    bottom: 80px;
    left: 0;
    right: 0;
  }
  .map{
    width: 100%;
    height: 1026rpx;
  }
  .map_text{
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0px;
    height: 80px;
    background: #fff;
    padding: 0 15px;
  }
  text{
    margin: 5px 0;
    display: block;
    font-size:12px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
  }
  .h1{
    margin: 15px 0;
    font-size:15px;
  }
  
  .map-1{
    width: 160rpx;
    height: 160rpx;
    border-radius: 120rpx;
    background-color: #4D8AD7;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    color:#ffffff;
    font-size: 34rpx;
    position: absolute;
    top:0rpx;
    right: 40rpx;
  }
  
  .map-1 image{
    width: 60rpx;
    height: 80rpx;
  }

.Distributionlist{
    margin-top: 20rpx;
    padding: 0 20rpx;   
    font-size: 28rpx;
    background-color: #fff;
}
.Distributionlist_top{
    padding-top: 34rpx;
    height: 146rpx;
    border-bottom: 2rpx solid #f5f5f5;
}
.Distributionlist_bottom{
    display: flex;
    justify-content: space-between;
    height: 100rpx;
    line-height: 100rpx;
}
.Distribution_btn{
    width: 194rpx;
    height: 72rpx;
    border-radius: 20rpx;
    color: #FFF;
    line-height: 72rpx;
    text-align: center;
    margin-top: 14rpx;
}

/* 导航条 */

page{
    background-color: #f5f5f5;
  }
.navbar{
    flex: none;
    display: flex;
    background: #fff;
  }
.navbar .item{
    position: relative;
    flex: auto;
    text-align: center;
    line-height: 24rpx;
}
.navbar .item.active{
    color: #2269d4;
    height: 80rpx;
}
.navbar .item.active:after{
    content: "";
    display: block;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 4rpx;
    background: #2269d4;
}