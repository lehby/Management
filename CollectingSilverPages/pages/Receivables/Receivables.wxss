.payment{
    padding: 0 20rpx;
    margin-top: 22rpx;
    background-color: #fff;
    font-size: 28rpx;
}
.payment_top{
    height: 90rpx;
    line-height: 90rpx;
    display:flex;
    justify-content: space-between;
    border-bottom:2rpx solid #f5f5f5;
}
.payment_Middle{
    padding: 26rpx 0;
    border-bottom:2rpx solid #f5f5f5;
}
.btnlist{
    padding: 14rpx 0;
    display:flex;
    justify-content: flex-end;
}
.btn_left{
    width: 164rpx;
    height: 72rpx;
    border: 2rpx solid;
    line-height: 72rpx;
    text-align: center;
    border-radius: 50rpx;
    margin-right: 20rpx;
}
.btn_right{
    width: 164rpx;
    height: 72rpx;
    line-height: 72rpx;
    text-align: center;
    border-radius: 50rpx;
    background-color: #2269d4;
    color: #fff;
}
/* 底部跳转 */

.fonter_head{
    display: flex;
  justify-content:space-around;
  background-color: white;
  width: 750rpx;
  position:fixed;
  bottom: 0;
  left: 0;
  padding: 6rpx;
}
.fonter{
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 25rpx;
}
.fonter image{
  width: 60rpx;
  height: 60rpx;
}