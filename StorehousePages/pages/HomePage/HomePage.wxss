.Palace{
    display: flex;
}
.box{
    width: 354rpx;
    height: 262rpx;
    margin: 10rpx 20rpx;
    background-color: #fff;
    border-radius: 8rpx;
    box-shadow: darkgrey 0px 0px 15px 5px;
}
.box>image{
    width: 104rpx;
    height: 104rpx;
    margin-left:120rpx;
    margin-top:40rpx;
}
.box>view{
    display: flex;
    justify-content: center;
}
/* 底部导航条 */
.fonter_head{
  display: flex;
  justify-content:space-around;
  background-color: white;
  width: 750rpx;
  position:fixed;
  bottom: 0;
  left: 0;
  padding: 6rpx;
}
.fonter{
  display: flex;
  flex-direction: column;
  font-size: 25rpx;
  align-items: center;
}
.fonter image{
  width: 60rpx;
  height: 60rpx;
}